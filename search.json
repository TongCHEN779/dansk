---
layout: null
permalink: /search.json
---

[
  {% assign selected_pages = site.pages | where_exp: "page", "page.permalink == '/ord_og_gram/adj/' or page.permalink == '/ord_og_gram/sub/' or page.permalink == '/ord_og_gram/verb/'" %}

  {% for page in selected_pages %}
    {
      "title": "{{ page.title | escape }}",
      "url": "{{ page.url | relative_url }}",
      "content": "{{ page.content | strip_html | strip_newlines | escape }}"
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
